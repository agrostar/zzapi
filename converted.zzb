    Send OTP:
        method: GET
        url: $(userserver)/userservice/otp/
        headers: []
        params:
            - { name: app, value: AgroStar }
            - { name: phoneNumber, value: $(phone) }

    Verify OTP:
        method: POST
        url: $(userserver)/userservice/otp/
        headers:
            - { name: Content-Type, value: application/json }
            - { name: X-Authorization-Token, value: $(anonAuthToken) }
        body: |
            {
              "otpToken": "$(otpToken)",
              "otp": "$(otp)"
            }

    CRM Login with username:
        method: POST
        url: $(userserver)/userservice/uservalidation/
        headers:
            - { name: Source, value: CSR }
        body:
            emailId: vasan.subramanian
            password: Gelusil1!
            rememberMe: false

    AgroWiki login with username:
        method: POST
        url: $(userserver)/userservice/login/
        headers:
            - { name: Source, value: CSR }
